{
  "manifest_version": 2,

  "name": "Chrome Navigator",
  "description": "for Fliggy Page Navigator",
  "version": "1.0",

  "browser_action": {
    "default_title": "快速跳转",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "http://*/",
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}